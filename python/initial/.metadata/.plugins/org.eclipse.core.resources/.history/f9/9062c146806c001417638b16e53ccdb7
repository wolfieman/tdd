import unittest

from primes import Primes

class PrimesTestCase(unittest.TestCase):
    """Tests for `primes.py`."""

    def test_is_five_prime(self):
        """Is five successfully determined to be prime?"""
        prime = Primes(5)
        self.assertTrue(prime.is_prime(5))

if __name__ == '__main__':
    unittest.main()